<div class="container">
  <div class="search center">
    <div class="wrapper">
      <p class="error-message"></p>
      <form id="searchform" method="post" action="/users/search">
        <input id="search-field" type="text" name="search" placeholder="Search..." value="{{query}}">
        <div class="go-button" onclick="executeSearch()">Go</div>
      </form>
    </div>
  </div>
  <section class="results-pane left">
    <aside class="saved-searches-slider">
      <p>Saved Searches</p>
    </aside>
    <aside class="saved-searches">
      <div class="center">
        <h2>Saved Searches <hr></h2>
        {{#each saved}}
        {{>savedsearch search=this}}
        {{/each}}
      </div>
        <p class="dismiss">X</p>
    </aside>
    <ul>
      {{#if results.length}}
      {{#each results}}
      <li>
        {{>searchresult url=this.images.standard_resolution.url alt=this.caption.text liked=this.user_has_liked}}
      </li>
      {{/each}}
      {{else}}
      <h3 class="no-result">No Results</h3>
      {{/if}}
    </ul>
  </section>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"></script>
  <script src="../js/dustin.js"></script>
</div>
